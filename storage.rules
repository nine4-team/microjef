rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /entries/{fileName} {
      allow read: if true;
      allow create: if request.resource.size < 10 * 1024 * 1024
                    && request.resource.contentType.matches('image/.*');
      allow update, delete: if false;
    }
  }
}
